	var myGoodsHistSize = "";
	var myActionHistSize = 0;	
	var delActionFlag = true;
	function deleteHistory(singleFlag) {
		
		var tabIdx = $(".his_tab_filter li.active").index();
		if(tabIdx == 0) {
			
			if(parseInt(myGoodsHistSize) > 0) {
				
				if(confirm("최근 본 상품 내역을 삭제하시겠습니까?")) {
					//최근본상품 삭제	
					setCookie("goods_history", "");
					$(".act_list2").empty();
					$(".his_tab_filter_con.goods .no_info").show();
					myGoodsHistSize = 0;
				}
			}
		} else if(tabIdx == 1) {
			
			if(singleFlag) {
				//활동내역 선택 삭제
				if(delActionFlag) {
					
					if(confirm("활동 내역을 삭제하시겠습니까?")) {
						
						delActionFlag = false;
						var $obj = $(event.target);
						$.ajax("/common/delActionHistory.do", {
							type : "post",
							dataType : "json",
							data : {"id" : $obj.attr("data-id"),
								    "type" : $obj.attr("data-type")},
							success : function(data) {
								if(data.resultCode == "0000") {
									
									$obj.parent().remove();	
									myActionHistSize = myActionHistSize - 1;
									if(myActionHistSize == 0) {
										
										$(".his_tab_filter_con.action .no_info").show();
									}
								} else {
									
									alert(data.resultMsg);
								}
							},
							error : function(x, o, e) { 
								//alert("서버 통신이 원활하지 않습니다.\n"+x.status + " : "+ o +" : "+e); 
							},
							complete : function() {
								delActionFlag = true;
							}
						});
					}
				}
			} else {
				//전체 활동내역 삭제
				if(parseInt(myActionHistSize) > 0) {
					
					if(confirm("전체 활동 내역을 삭제하시겠습니까?")) {
						
						setCookie("action_history", "");
						$(".act_list").empty();
						$(".his_tab_filter_con.action .no_info").show();
						myActionHistSize = 0;
					}
				}
			}
		}
	}//deleteHistory end
	
	//활동내역 가져오기
	var actionFlag = true;
	var callFlag = false;
	function getActionHistory(idx) {
		
		if(!callFlag) {
			
			if(actionFlag) {
				actionFlag = false;
				
				$.ajax("/common/ActionHistoryListAjax.do", {
					type : "post",
					dataType : "html",
					success : function(data) {
						
						$(".act_list").html(data);
						myActionHistSize = $(".act_list li").length; 
						if(myActionHistSize == 0) {
							
							$(".his_tab_filter_con.action .no_info").show();
						}
						
						$('.his_tab_filter li').removeClass('active').eq(idx).addClass('active');
						$('.his_tab_filter_con').hide().eq(idx).show();
						callFlag = true;
					},
					error : function(x, o, e) { 
						actionFlag = true;
						//alert("서버 통신이 원활하지 않습니다.\n"+x.status + " : "+ o +" : "+e); 
					}
				});
			}
		} else {
			
			$('.his_tab_filter li').removeClass('active').eq(idx).addClass('active');
			$('.his_tab_filter_con').hide().eq(idx).show();
		}
	}//getActionHistory end
	
