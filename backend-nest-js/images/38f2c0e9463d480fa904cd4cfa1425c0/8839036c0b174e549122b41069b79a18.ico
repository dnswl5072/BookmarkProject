	}else if(navigator.userAgent.search("Android") > -1 && isChrome) {//android chrome
			top.window.location = "intent://?returnUrl=http%3A%2F%2Fm.akmall.com%3FisAkApp%3DAndroid#Intent;scheme=akmall;package=com.ak.android.akmall;end;";
	}else if(navigator.userAgent.search("Android") > -1) { //android
		var iframe = document.createElement("iframe");
		iframe.style.visibility = "hidden";
		iframe.style.height = 0 ;
		iframe.src =  "akmall://?returnUrl=http%3A%2F%2Fm.akmall.com%3FisAkApp%3DAndroid";
		iframe.id = "androidIframe";
		iframe.onload = function() {
			window.location.href = "market://details?id=com.ak.android.akmall";
		};
		document.body.appendChild(iframe);
	}else {
		alert("지원하지 않는 기기입니다.");
	}
	return false;
}
</script>
   
