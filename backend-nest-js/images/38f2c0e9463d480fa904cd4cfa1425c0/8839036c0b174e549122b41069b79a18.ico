 <script>
 require(["main"],function() {
	 if($("#siteCode").val() == "MOB") {
		 var recopick_uid = getCookie("recopick_uid");
		 if(recopick_uid != "") {
			 var recopick_url = "https://api.recopick.com/v1/recommendations/user/1102/"+recopick_uid+"?pageId=3244364&limit=10&field=meta&type=realtime";
			 if("false" == "true") {
				 recopick_url = recopick_url+"&mid=f36502be1d10c0f5";	 
			 }
			 
			 $.ajax({
			     url : recopick_url,
			     dataType : "jsonp",
			     success : function(data){
			    	 var json = data;
			    	 
			    	 if(json.length < 15) {
		    	         
			    		 var limit = 15 - json.length; 
			    		 recopick_url = "https://api.recopick.com/v1/recommendations/user/1102/"+recopick_uid+"?pageId=3244452&limit=" + limit + "&field=meta&type=view2";
			    		 if("false" == "true") {
		    	         	recopick_url = recopick_url+"&mid=f36502be1d10c0f5";
			    		 }	 
		    	         
			    		 $.ajax({
				    	     url : recopick_url,
				    	     dataType : "jsonp",
				    	     success : function(data2){
				    	    	 
				    	    	 $(data2).each(function() {
				    	    		 json.push(this);	 
				    	    	 });
				    	    	 
				    	    	 recopicWrite(json);
				    	     }
				    	 });
			    	 } else {
			    		 recopicWrite(json);
			    	 }

			     }
			 });	
		 }
	 }	 
});

 function recopicWrite(data) {
	 var $ul = $(".mostview_slide");
	 var $li = "";
	 
	 if(data.length > 0) {
		 $ul.parents(".main_wrap").show();
	 }
	 
	 $(data).each(function(index) {
		 if(index%3 == 0) {
			$li = $('<li class="swiper-slide">');
			$li.appendTo($ul); 
		 } 
		 var $clicklog_redirect_link = $("<a>").attr("href", this.clicklog_redirect_link);
		 var $div = $("<div>");
		 
		 var src = "";
		 
		 if(this.image != undefined && this.image.src != undefined) {
		 
			 src = this.image.src;
			 
		 } else {
		 
			 src = "/resources/images/noImage/noimg.gif";
		 }
	 
		 var $img = $("<img>").attr({"src" : src}).error(function() {
			  $(this).attr("src", "/resources/images/noImage/noimg.gif");
		 });
	 
		 if(this.sale_price == null) this.sale_price = this.price;	 
		 var $title = $("<span>").text(this.title);
		 var $salePrice = $("<strong>").html(comma(Number(this.sale_price)) + "<i>원<i>");
		 var $price = $("<del>").text(comma(Number(this.price)) + "원");
		 
		 if(this.price == this.sale_price){
             $price.text("").addClass("none");
      	 }
		 $div.append($img, $title, $price, $salePrice)
	 	 .appendTo($clicklog_redirect_link);
		 $li.append($clicklog_redirect_link);
		 
	 });
	 
	 setTimeout(function() {
		 swiperSlide.item('swiper-mostviewError','swiper-page');	 
	 }, 300);
	 
 }
 
 </script>    
