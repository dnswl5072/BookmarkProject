<script type="text/javascript">
getCookie=function(Security){
	var cookieName='Security';
    var cookieValue = $.cookie(cookieName);
    //alert("확인:::layout.jsp:::"+cookieValue);
    if(cookieValue==null && cookieValue=="no"){
        $.cookie(Security,"no",{path:'/'});
        TouchEnNxConfig.use.nxkey=false;
        TouchEnNxConfig.use.nxweb=false;
        TouchEnNx.init();
        return true;
    }else if(cookieValue=="yes"){
    	//alert("보안적용::layout.jsp");
    	TouchEnNxConfig.use.nxkey=true;
        TouchEnNxConfig.use.nxweb=true;
    	TouchEnNx.init();
        return true;
    }
    return false;
};
</script>	
